<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.web_aoplogging.dao.user.LoginMapper">
    <select id="selectByPrimaryKey" resultType="com.demo.web_aoplogging.model.po.Login" parameterType="java.lang.String" >
        SELECT 
            *
        FROM 
            login
        WHERE 
            username = #{username,jdbcType=VARCHAR}
    </select>
</mapper>